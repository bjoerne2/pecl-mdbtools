  <refentry id="function.mdb-table-close">
   <refnamediv>
    <refname>mdb_table_close</refname>
    <refpurpose>Close an opened table</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>mdb_table_close</methodname>
      <methodparam><type>resource</type><parameter>table</parameter></methodparam>
     </methodsynopsis>
     <para>
    &lt;para&gt;
      Close access to &lt;parameter&gt;table&lt;/parameter&gt;.
    &lt;/para&gt;
     </para>

   </refsect1>
  </refentry>

